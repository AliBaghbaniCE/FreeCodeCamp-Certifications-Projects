{
  "name": "freecodecamp-certifications-projects",
  "version": "1.0.0",
  "keywords": ["freeCodeCamp", "certification", "project"],
  "description": "All of the projects which I have done to earn my certifications from the freeCodeCamp website.",
  "main": "README.md",
  "config": {
    "project11": "1-HTML-CSS\\1-Survey-Form",
    "project12": "1-HTML-CSS\\2-Tribute-Page",
    "project13": "1-HTML-CSS\\3-Technical-Documentation-Page",
    "project14": "1-HTML-CSS\\4-Product-Landing-Page"
  },
  "scripts": {
    "watchProjects": "live-server",
    "watchProject11": "live-server --open=%npm_package_config_project11%\\index.html",
    "watchProject12": "live-server --open=%npm_package_config_project12%\\index.html",
    "watchProject13": "live-server --open=%npm_package_config_project13%\\index.html",
    "watchProject14": "live-server --open=%npm_package_config_project14%\\index.html",
    "watchPretty": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "watchCss11": "sass --watch %npm_package_config_project11%\\scss:%npm_package_config_project11%\\css",
    "watchCss12": "sass --watch %npm_package_config_project12%\\scss:%npm_package_config_project12%\\css",
    "watchCss13": "sass --watch %npm_package_config_project13%\\scss:%npm_package_config_project13%\\css",
    "watchCss14": "sass --watch %npm_package_config_project14%\\scss:%npm_package_config_project14%\\css",
    "PrettyRootDir": "prettier --write *.*",
    "prettyProject11": "prettier --write %npm_package_config_project11%",
    "prettyProject12": "prettier --write %npm_package_config_project12%",
    "prettyProject13": "prettier --write %npm_package_config_project13%",
    "prettyProject14": "prettier --write %npm_package_config_project14%",
    "buildCss11": "sass %npm_package_config_project11%\\scss:%npm_package_config_project11%\\css --style=compressed --charset --source-map-urls=relative",
    "buildCss12": "sass %npm_package_config_project12%\\scss:%npm_package_config_project12%\\css --style=compressed --charset --source-map-urls=relative",
    "buildCss13": "sass %npm_package_config_project13%\\scss:%npm_package_config_project13%\\css --style=compressed --charset --source-map-urls=relative",
    "buildCss14": "sass %npm_package_config_project14%\\scss:%npm_package_config_project14%\\css --style=compressed --charset --source-map-urls=relative",
    "prefixCss11": "postcss %npm_package_config_project11%\\css\\*.css --use autoprefixer -d %npm_package_config_project11%\\css",
    "prefixCss12": "postcss %npm_package_config_project12%\\css\\*.css --use autoprefixer -d %npm_package_config_project12%\\css",
    "prefixCss13": "postcss %npm_package_config_project13%\\css\\*.css --use autoprefixer -d %npm_package_config_project13%\\css",
    "prefixCss14": "postcss %npm_package_config_project14%\\css\\*.css --use autoprefixer -d %npm_package_config_project14%\\css",
    "buildProject11": "npm run PrettyRootDir && npm run prettyProject11 && npm run buildCss11 && npm run prefixCss11",
    "buildProject12": "npm run PrettyRootDir && npm run prettyProject12 && npm run buildCss12 && npm run prefixCss12",
    "buildProject13": "npm run PrettyRootDir && npm run prettyProject13 && npm run buildCss13 && npm run prefixCss13",
    "buildProject14": "npm run PrettyRootDir && npm run prettyProject14 && npm run buildCss14 && npm run prefixCss14",
    "buildAll": "npm run buildProject11 && npm run buildProject12 && npm run buildProject13 &&  npm run buildProject14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliBaghbaniCE/FreeCodeCamp-Certifications-Projects.git"
  },
  "author": {
    "name": "Ali Baghbani",
    "email": "alibaghbanice@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AliBaghbaniCE/FreeCodeCamp-Certifications-Projects/issues"
  },
  "homepage": "https://github.com/AliBaghbaniCE/FreeCodeCamp-Certifications-Projects#readme",
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "live-server": "1.2.2",
    "onchange": "7.1.0",
    "postcss": "8.4.14",
    "postcss-cli": "10.0.0",
    "prettier": "2.7.1",
    "sass": "1.53.0"
  }
}
